{
  "kind": "collectionType",
  "collectionName": "tokens",
  "info": {
    "name": "Token",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "token": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 40,
      "regex": "^[^\\s]+(\\s+[^\\s]+)*$",
      "private": true
    },
    "user": {
      "unique": true,
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
